{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "antd": "^5.1.7",
    "copy-to-clipboard": "^3.3.3",
    "ethers": "^5.4.7",
    "identicon.js": "^2.3.3",
    "js-base64": "^3.7.5",
    "pubsub-js": "^1.9.4",
    "react": "17.x",
    "react-dom": "17.x",
    "react-toastify": "^8.2.0",
    "umi": "^3.5.18",
    "umi-plugin-keep-alive": "^0.0.1-beta.29",
    "walletconnect": "^1.6.6",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.36",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
